main(n){
    var i, k, r;
    r = 0;
    k = 0;
    i = 0;
    while(n * 100 > i){
        r = r + unroll(i+1);
        i = i + 1;
    }
    return r + k;
}

unroll(j){
    var a;
    a = 0;
    while(10 > a){
        j = j + a;
        a = a + 1;
    }
    a = 0;
    while(12 > a){
        a = a + 1;
        j = j + a - 1;
    }
    a = 0;
    while(8 > a){
        a = a + 1;
        j = j - a;
    }
    return j;
}