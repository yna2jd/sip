main(n) {
  var i, r, x;
  i = 0;
  r = 1;
  while (1000 > i) { 
    r = getPlus(r,i); 
    i = getPlus(i,1); 
  }
  i=0;
  while (10 > i) { 
    r = getPlus(r,r);
    r = getDiv(r,i); 
    i = getPlus(i,1); 
  }
  i = 0;
  x = 1;
  while (1000 > i) { 
    x = getPlus(x,i); 
    i = getPlus(i,1); 
  }
  i=1;
  while (10 > i) { 
    x = getPlus(x,x);
    x = getDiv(x,i); 
    i = getPlus(i,1); 
  }
  return getPlus(r,x);
}

getPlus(n,m) {
  return n+m;
}

getDiv(n,m) {
  return n/m;
}
